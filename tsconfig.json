{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react",
    "lib": ["dom", "es2015", "es2017"],
    "esModuleInterop": true,
    "strictNullChecks": true
  },
  "include": ["./src/**/*", "./definitions/*", "./gatsby-*.ts"]
}
